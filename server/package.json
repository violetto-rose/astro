{
  "name": "rakshalokam",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev:dashboard": "vite --clearScreen false",
    "dev": "concurrently --kill-others-on-fail npm:dev:*",
    "build": "tsc",
    "build:dashboard": "vite build",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "dependencies": {
    "@vendure/asset-server-plugin": "3.5.3",
    "@vendure/core": "3.5.3",
    "@vendure/dashboard": "3.5.3",
    "@vendure/email-plugin": "3.5.3",
    "@vendure/graphiql-plugin": "3.5.3",
    "dotenv": "17.3.1",
    "pg": "8.18.0",
    "rakshalokam-monorepo": "file:..",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@vendure/cli": "3.5.3",
    "concurrently": "9.2.1",
    "typescript": "5.8.2"
  }
}
